<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.zjh.dao.mapper.CustomerProfileMapper">

	<select id="getCustomerProfileInfo" resultType="com.cn.zjh.dao.output.CustomerProfileDaoOutput">
		select 
		a.user_id as userId,
		a.user_name as userName,
		a.password as pwd,
		a.status as status
		from
		customer_profile_info a
	</select>

</mapper>